{
  "name": "romeoz/rock-request",
  "description": "HTTP request library for PHP",
  "keywords": [
    "request",
    "http request"
  ],
  "type": "library",
  "license": "MIT",
  "support": {
    "email": "serggalka@gmail.com",
    "issues": "https://github.com/romeOz/rock-request/issues",
    "source": "https://github.com/romeOz/rock-request"
  },
  "authors": [
    {
      "name": "Ruslan",
      "email": "serggalka@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "romeoz/rock-base": "*@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7.0",
    "romeoz/rock-sanitize": "*@dev"
  },
  "suggest": {
    "romeoz/rock-sanitize": "required for sanitizing http-variables"
  },
  "autoload": {
    "psr-4": {
      "rock\\request\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.14-dev"
    }
  }
}